    "class-validator": "^0.14.3",
    "graphql": "^16.12.0",
    "graphql-upload": "^17.0.0",
    "moment": "^2.30.1",
    "mongoose": "^9.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
