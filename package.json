  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:batch": "nest start meomul-batch --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/apps/meomul-api/main",
    "start:prod:batch": "NODE_ENV=production node dist/apps/meomul-batch/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
